<?xml version="1.0" encoding="UTF-8"?>
<filter-conf>
    <version>20220902</version>
    <toggle>1</toggle>
    <features>

        <feature name="BindCore" enable="1">
            <!-- big core index, start from 0 -->
            <bigcore_id>4</bigcore_id>
            <bigcore_id>5</bigcore_id>
            <bigcore_id>6</bigcore_id>
            <bigcore_id>7</bigcore_id>
        </feature>

        <feature name="OplusDEE" enable="1">
            <DC>enable</DC>
            <application name="com.coloros.video">
                <scene type="Application">
                    <action>
                        <mode>AD</mode>
                    </action>
                </scene>
            </application>
            <application name="com.oneplus.camera">
                <scene type="Application">
                    <action>
                        <mode>AD</mode>
                    </action>
                </scene>
            </application>
            <application name="com.coloros.gallery3d">
                <scene type="Application">
                    <action>
                        <mode>AD</mode>
                    </action>
                </scene>
            </application>
        </feature>


        <feature name="curtain" enable="1">
            <application name="com.coloros.gallery3d">
                <scene type="FuzzyLayer">
                    <layer>Gallery</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
            </application>
            <application name="tv.danmaku.bili">
                <scene type="FuzzyLayer">
                    <layer>VideoDetailsActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
                <scene type="FuzzyLayer">
                    <layer>LiveRoomActivityV3</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
                <scene type="FuzzyLayer">
                    <layer>StoryVideoActivity</layer>
                    <action>
                        <mode>Normal</mode>
                    </action>
                </scene>
            </application>
            <application name="com.hunantv.imgo.activity">
                <scene type="FuzzyLayer">
                    <layer>VodPlayerPageActivity</layer>
                    <action>
                        <mode>Normal</mode>
                    </action>
                </scene>
            </application>
            <application name="com.tencent.qqlive">
                <scene type="FuzzyLayer">
                    <layer>VideoDetailActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
            </application>
            <application name="com.youku.phone">
                <scene type="FuzzyLayer">
                    <layer>DetailActivity</layer>
                    <action>
                        <mode>SurfaceView</mode>
                    </action>
                </scene>
            </application>
        </feature>

        <feature name="DispKeventThread" enable="1"/>

        <feature name="hecate" enable="1"/>

    </features>
</filter-conf>
